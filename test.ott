
frame_count = 0

function start_frame() {
    if (frame_count >= 1000) {
        frame_count = 0
    }

    frame_count = frame_count + 1
}

function start_game() {
    running = true
    quit = false

    while (running != false and quit == false) {
        start_frame()

        print(frame_count)
    }

    return 
}

function entry() {
    start_game()
}
